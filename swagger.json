{
  "openapi": "3.0.1",
  "info": {
    "title": "CrescimentoExponencial",
    "version": "v1"
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    },
    {
      "url": "http://www.crescimentoexponencial.com"
    }
  ],
  "paths": {
    "/TreinoControler": {
      "get": {
        "tags": [
          "TreinoControler"
        ],
        "summary": "Obter todos os treinos",
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      },
      "post": {
        "tags": [
          "TreinoControler"
        ],
        "summary": "Cadastra novo treino",
        "requestBody": {
          "description": "",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TreinoDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TreinoDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TreinoDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TreinoDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true,
            "example": "Supino reto"
          },
          "repeticoes": {
            "type": "integer",
            "format": "int32",
            "example": 15
          }
        },
        "additionalProperties": false
      }
    }
  }
}